{
    "provider": [
        {
            "azurerm": {
                "features": [
                    {}
                ]
            }
        }
    ],
    "resource": [
        {
            "azurerm_resource_group": {
                "rg": {
                    "location": "${var.resource_group_location}",
                    "name": "${var.resource_group_name}"
                }
            }
        },
        {
            "azurerm_virtual_network": {
                "vnet": {
                    "address_space": [
                        "10.0.0.0/16"
                    ],
                    "location": "${azurerm_resource_group.rg.location}",
                    "name": "vnet_application_gateway",
                    "resource_group_name": "${azurerm_resource_group.rg.name}"
                }
            }
        },
        {
            "azurerm_public_ip": {
                "vmpubip": {
                    "allocation_method": "Static",
                    "location": "${azurerm_resource_group.rg.location}",
                    "name": "vmpip",
                    "resource_group_name": "${azurerm_resource_group.rg.name}",
                    "sku": "Standard"
                }
            }
        },
        {
            "azurerm_public_ip": {
                "vm1pubip": {
                    "allocation_method": "Static",
                    "location": "${azurerm_resource_group.rg.location}",
                    "name": "vm1pip",
                    "resource_group_name": "${azurerm_resource_group.rg.name}",
                    "sku": "Standard"
                }
            }
        },
        {
            "azurerm_public_ip": {
                "gatwayip": {
                    "allocation_method": "Static",
                    "location": "${azurerm_resource_group.rg.location}",
                    "name": "gatway-pip",
                    "resource_group_name": "${azurerm_resource_group.rg.name}",
                    "sku": "Standard"
                }
            }
        },
        {
            "azurerm_subnet": {
                "subnetvm": {
                    "address_prefixes": [
                        "10.0.1.0/24"
                    ],
                    "name": "subnetforvm",
                    "resource_group_name": "${azurerm_resource_group.rg.name}",
                    "virtual_network_name": "${azurerm_virtual_network.vnet.name}"
                }
            }
        },
        {
            "azurerm_subnet": {
                "subnetapp": {
                    "address_prefixes": [
                        "10.0.2.0/24"
                    ],
                    "name": "subnetforapp",
                    "resource_group_name": "${azurerm_resource_group.rg.name}",
                    "virtual_network_name": "${azurerm_virtual_network.vnet.name}"
                }
            }
        },
        {
            "azurerm_subnet": {
                "appsubnet": {
                    "address_prefixes": [
                        "10.0.3.0/24"
                    ],
                    "name": "subnetforlb",
                    "resource_group_name": "${azurerm_resource_group.rg.name}",
                    "virtual_network_name": "${azurerm_virtual_network.vnet.name}"
                }
            }
        },
        {
            "azurerm_network_interface": {
                "nic1": {
                    "ip_configuration": [
                        {
                            "name": "internal",
                            "private_ip_address_allocation": "Dynamic",
                            "public_ip_address_id": "${azurerm_public_ip.vmpubip.id}",
                            "subnet_id": "${azurerm_subnet.subnetvm.id}"
                        }
                    ],
                    "location": "${azurerm_resource_group.rg.location}",
                    "name": "example-nic-vm1",
                    "resource_group_name": "${azurerm_resource_group.rg.name}"
                }
            }
        },
        {
            "azurerm_network_interface": {
                "nic2": {
                    "ip_configuration": [
                        {
                            "name": "internal",
                            "private_ip_address_allocation": "Dynamic",
                            "public_ip_address_id": "${azurerm_public_ip.vm1pubip.id}",
                            "subnet_id": "${azurerm_subnet.subnetvm.id}"
                        }
                    ],
                    "location": "${azurerm_resource_group.rg.location}",
                    "name": "example-nic-vm2",
                    "resource_group_name": "${azurerm_resource_group.rg.name}"
                }
            }
        },
        {
            "azurerm_network_security_group": {
                "nsg1": {
                    "location": "${azurerm_resource_group.rg.location}",
                    "name": "nsg2",
                    "resource_group_name": "${azurerm_resource_group.rg.name}",
                    "security_rule": [
                        {
                            "access": "Allow",
                            "description": null,
                            "destination_address_prefix": "*",
                            "destination_address_prefixes": null,
                            "destination_application_security_group_ids": null,
                            "destination_port_range": "*",
                            "destination_port_ranges": null,
                            "direction": "Inbound",
                            "name": "All",
                            "priority": 200,
                            "protocol": "Tcp",
                            "source_address_prefix": "*",
                            "source_address_prefixes": null,
                            "source_application_security_group_ids": null,
                            "source_port_range": "*",
                            "source_port_ranges": null
                        }
                    ]
                }
            }
        },
        {
            "azurerm_network_security_group": {
                "nsg2": {
                    "location": "${azurerm_resource_group.rg.location}",
                    "name": "nsg3",
                    "resource_group_name": "${azurerm_resource_group.rg.name}",
                    "security_rule": [
                        {
                            "access": "Allow",
                            "description": null,
                            "destination_address_prefix": "*",
                            "destination_address_prefixes": null,
                            "destination_application_security_group_ids": null,
                            "destination_port_range": "*",
                            "destination_port_ranges": null,
                            "direction": "Inbound",
                            "name": "All",
                            "priority": 200,
                            "protocol": "Tcp",
                            "source_address_prefix": "*",
                            "source_address_prefixes": null,
                            "source_application_security_group_ids": null,
                            "source_port_range": "*",
                            "source_port_ranges": null
                        }
                    ]
                }
            }
        },
        {
            "azurerm_network_interface_security_group_association": {
                "sg": {
                    "network_interface_id": "${azurerm_network_interface.nic1.id}",
                    "network_security_group_id": "${azurerm_network_security_group.nsg1.id}"
                }
            }
        },
        {
            "azurerm_network_interface_security_group_association": {
                "sg1": {
                    "network_interface_id": "${azurerm_network_interface.nic2.id}",
                    "network_security_group_id": "${azurerm_network_security_group.nsg2.id}"
                }
            }
        },
        {
            "azurerm_linux_virtual_machine": {
                "vm1": {
                    "admin_password": "Rupali152012345",
                    "admin_username": "azureuser",
                    "computer_name": "myvm",
                    "disable_password_authentication": false,
                    "location": "${azurerm_resource_group.rg.location}",
                    "name": "example-machine",
                    "network_interface_ids": [
                        "${azurerm_network_interface.nic1.id}"
                    ],
                    "os_disk": [
                        {
                            "caching": "ReadWrite",
                            "name": "osdist1",
                            "storage_account_type": "Standard_LRS"
                        }
                    ],
                    "resource_group_name": "${azurerm_resource_group.rg.name}",
                    "size": "Standard_DS1_v2",
                    "source_image_reference": [
                        {
                            "offer": "0001-com-ubuntu-server-jammy",
                            "publisher": "Canonical",
                            "sku": "22_04-lts-gen2",
                            "version": "latest"
                        }
                    ]
                }
            }
        },
        {
            "azurerm_linux_virtual_machine": {
                "vm2": {
                    "admin_password": "Rupali152012345",
                    "admin_username": "azureuser",
                    "computer_name": "myvm",
                    "disable_password_authentication": false,
                    "location": "${azurerm_resource_group.rg.location}",
                    "name": "example-machine1",
                    "network_interface_ids": [
                        "${azurerm_network_interface.nic2.id}"
                    ],
                    "os_disk": [
                        {
                            "caching": "ReadWrite",
                            "name": "osdist2",
                            "storage_account_type": "Standard_LRS"
                        }
                    ],
                    "resource_group_name": "${azurerm_resource_group.rg.name}",
                    "size": "Standard_DS1_v2",
                    "source_image_reference": [
                        {
                            "offer": "0001-com-ubuntu-server-jammy",
                            "publisher": "Canonical",
                            "sku": "22_04-lts-gen2",
                            "version": "latest"
                        }
                    ]
                }
            }
        },
        {
            "azurerm_application_gateway": {
                "network": {
                    "backend_address_pool": [
                        {
                            "ip_addresses": [
                                "${azurerm_network_interface.nic1.private_ip_address}",
                                "${azurerm_network_interface.nic2.private_ip_address}"
                            ],
                            "name": "pool"
                        }
                    ],
                    "backend_http_settings": [
                        {
                            "cookie_based_affinity": "Disabled",
                            "name": "httpsetting",
                            "path": "",
                            "port": 80,
                            "protocol": "Http",
                            "request_timeout": 1
                        }
                    ],
                    "frontend_ip_configuration": [
                        {
                            "name": "forntip",
                            "public_ip_address_id": "${azurerm_public_ip.gatwayip.id}"
                        }
                    ],
                    "frontend_port": [
                        {
                            "name": "fornt-port",
                            "port": 80
                        }
                    ],
                    "gateway_ip_configuration": [
                        {
                            "name": "my-gateway-ip-configuration",
                            "subnet_id": "${azurerm_subnet.subnetapp.id}"
                        }
                    ],
                    "http_listener": [
                        {
                            "frontend_ip_configuration_name": "forntip",
                            "frontend_port_name": "fornt-port",
                            "name": "listner",
                            "protocol": "Http"
                        }
                    ],
                    "location": "${azurerm_resource_group.rg.location}",
                    "name": "example-appgateway",
                    "request_routing_rule": [
                        {
                            "backend_address_pool_name": "pool",
                            "backend_http_settings_name": "httpsetting",
                            "http_listener_name": "listner",
                            "name": "rule1",
                            "priority": 2,
                            "rule_type": "Basic"
                        }
                    ],
                    "resource_group_name": "${azurerm_resource_group.rg.name}",
                    "sku": [
                        {
                            "capacity": 1,
                            "name": "Standard_v2",
                            "tier": "Standard_v2"
                        }
                    ]
                }
            }
        }
    ]
}